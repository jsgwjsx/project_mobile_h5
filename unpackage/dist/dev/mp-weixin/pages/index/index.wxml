<view><view class="log_pic"><image src="../../static/snow.jpg" mode></image></view><view class="out"><view class="row"><input class="border" type="text" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['message']]]]]}}" value="{{message.username}}" bindinput="__e"/></view><view class="row"><input class="border" type="text" placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['$0','tel','$event',[]],['message']]]]]}}" value="{{message.tel}}" bindinput="__e"/></view><view class="row"><input class="border" type="text" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','code','$event',[]],['message']]]]]}}" value="{{message.code}}" bindinput="__e"/></view></view><view class="buttonregister"><button size="mini" type="primary" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button></view><view class="buttonlog"><button size="mini" type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登录</button></view></view>